{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "variables":{
        "baseTemplateLink": "[uri(deployment().properties.templateLink.uri,'Networking/Deploymenturi.json')]",
       "anotherBaseTemplateLink": "[uri(deployment().properties.templateLink.uri,'Networking/Deploymenturi.json')]",
         "childTemplateLink":"[replace(variables('anotherBaseTemplateLink'),'Networking', 'MasterTemplate')]"
        },
    "resources": [],
    "outputs": {
        "baseuriOutput": {
            "type": "string",
            "value": "[uriComponentToString(variables('baseTemplateLink'))]"
        },
         "uriOutput": {
            "type": "string",
            "value": "[variables('childTemplateLink')]"
        }
    
    }
}



