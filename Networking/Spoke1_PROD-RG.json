{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworks_Spoke1_PROD_VNET_name": {
            "defaultValue": "Spoke1_PROD-VNET",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2018-10-01",
            "name": "[parameters('virtualNetworks_Spoke1_PROD_VNET_name')]",
            "location": "canadacentral",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.4.0.0/24"
                    ]
                },
                "subnets": [
                    {
                        "name": "Spoke1_PROD-SNET-App",
                        "properties": {
                            "addressPrefix": "10.4.0.0/25",
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "Spoke1_PROD-SNET-DB",
                        "properties": {
                            "addressPrefix": "10.4.0.128/25",
                            "serviceEndpoints": [],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2018-10-01",
            "name": "[concat(parameters('virtualNetworks_Spoke1_PROD_VNET_name'), '/Spoke1_PROD-SNET-App')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_Spoke1_PROD_VNET_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.4.0.0/25",
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2018-10-01",
            "name": "[concat(parameters('virtualNetworks_Spoke1_PROD_VNET_name'), '/Spoke1_PROD-SNET-DB')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_Spoke1_PROD_VNET_name'))]"
            ],
            "properties": {
                "addressPrefix": "10.4.0.128/25",
                "serviceEndpoints": [],
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        }
    ]
}